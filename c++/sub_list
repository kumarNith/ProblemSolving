#include <iostream>
#include <string>
#include <vector>

using namespace std;

void sublist_helper(vector<string> lst, vector<string> sub_lst){
    if(lst.count == 0){
        cout<<sub_lst;
    }
    
}

void sublist(vector<string> lst){
    vector<string> sub_lst = {""};
    sublist_helper(lst, vector<string> sub_lst)
}

int main()
{
    vector<string> lst = {"father","mother","son","daughter"};
    sublist(lst);
    
    //cout<<"Hello World";

    //return 0;
}
